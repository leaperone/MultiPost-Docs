  "tags": [],
  "paths": {
    "/v1/search": {
      "post": {
        "summary": "搜索",
        "deprecated": false,
        "description": "提供搜索关键词，返回搜索结果",
        "tags": [],
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "q": {
                    "type": "string",
                    "title": "关键词",
                    "description": "搜索关键词"
                  },
                  "num": {
                    "type": "integer",
                    "default": 10,
                    "title": "返回数量",
                    "description": "数量越多消耗Credit越多"
                  },
                  "page": {
                    "type": "string",
                    "title": "分页"
                  },
                  "site": {
                    "type": "string",
                    "title": "指定搜索域",
                    "description": "只搜索某网址或某域下面的内容",
                    "default": "https://multipost.app"
                  },
                  "hl": {
                    "type": "string",
                    "title": "指定搜索语言",
                    "description": "两位，如en、fr、ja、zh-cn"
                  }
                },
                "required": [
                  "q"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "title": {
                            "type": "string"
                          },
                          "url": {
                            "type": "string"
                          },
                          "description": {
                            "type": "string"
                          },
                          "content": {
                            "type": "string"
                          },
                          "favicon": {
                            "type": "string"
                          }
                        },
                        "required": [
                          "title",
                          "url"
                        ]
                      }
                    },
                    "meta": {
                      "type": "object",
                      "properties": {
                        "credits": {
                          "type": "number"
                        }
                      },
                      "required": [
                        "credits"
                      ]
                    }
                  },
                  "required": [
                    "success",
                    "data",
                    "meta"
                  ]
                },
                "example": {
                  "success": true,
                  "data": [
                    {
                      "title": "Open Source Social Media Publishing Tool",
                      "url": "https://multipost.app/"
                    },
                    {
                      "title": "Vista® MultiPost Therapy Collar - Aspen Medical Products",
                      "url": "https://www.aspenmp.com/products/vistar-multipost-therapy-collar"
                    },
                    {
                      "title": "MultiPost - Chrome Web Store",
                      "url": "https://chromewebstore.google.com/detail/multipost/dhohkaclnjgcikfoaacfgijgjgceofih?hl=en"
                    }
                  ],
                  "meta": {
                    "credits": 0.0020004
                  }
                }
              }
            },
            "headers": {}
          }
        },
        "security": [
          {
            "bearer": []
          }
        ]
      }
    },
    "/v1/reader": {
      "post": {
        "summary": "网页内容爬取",
        "deprecated": false,
        "description": "提供搜索关键词，返回搜索结果",
        "tags": [],
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "url": {
                    "type": "string",
                    "title": "网页URL"
                  },
                  "targetSelector": {
                    "anyOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    ],
                    "title": "指定目标元素",
                    "description": "元素选择器，如 .class, #id"
                  },
                  "removeSelector": {
                    "anyOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    ],
                    "title": "排除元素",
                    "description": "用于排除页眉、页脚等"
                  },
                  "waitForSelector": {
                    "anyOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "array",
                        "items": {
                          "type": "string"
                        }
                      }
                    ],
                    "title": "等待元素出现",
                    "description": "在该元素出现后才返回"
                  },
                  "viewport": {
                    "type": "object",
                    "properties": {
                      "width": {
                        "type": "integer",
                        "title": "宽"
                      },
                      "height": {
                        "type": "integer",
                        "title": "高"
                      }
                    },
                    "title": "视口",
                    "required": [
                      "height",
                      "width"
                    ],
                    "description": "用于响应式"
                  },
                  "returnFormat": {
                    "type": "string",
                    "title": "返回内容格式",
                    "description": "默认Markdown",
                    "default": "https://multipost.app",
                    "enum": [
                      "markdown",
                      "html",
                      "text"
                    ]
                  }
                },
                "required": [
                  "url"
                ]
              }
            }
          }
        },
